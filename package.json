{
  "name": "oswp",
  "description": "The Open Source Wedding Project - A modern, open-source wedding planning and RSVP management system",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "SKIP_ENV_VALIDATION=true next build",
    "build:analyze": "SKIP_ENV_VALIDATION=true next build --analyze",
    "build:turbo": "NODE_OPTIONS='--trace-uncaught' SKIP_ENV_VALIDATION=true next build --turbopack",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev",
    "dev:turbo": "NODE_OPTIONS='--trace-uncaught' next dev --turbopack",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "SKIP_ENV_VALIDATION=true jest",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "postinstall": "prisma generate",
    "start": "next start",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.937.0",
    "@daveyplate/better-auth-ui": "^3.2.11",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "7.0.0",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.90.10",
    "@trpc/client": "^11.7.2",
    "@trpc/next": "^11.7.2",
    "@trpc/react-query": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "better-auth": "^1.4.1",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "next": "^15.1.6",
    "next-auth": "^4.24.13",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.1",
    "react-cropper": "^2.3.3",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.37.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.1.0",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.5",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "7.0.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "npx eslint --fix",
      "npx prettier --write"
    ]
  },
  "ct3aMetadata": {
    "initVersion": "7.32.0"
  },
  "packageManager": "npm@10.9.0"
}
